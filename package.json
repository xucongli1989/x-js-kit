{
  "name": "x-js-kit",
  "version": "2.2.0",
  "description": "This is my javascript toolkit. ",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "webpack  --mode=development --progress",
    "dist": "webpack  --mode=production --progress && tsc --emitDeclarationOnly && npm run test",
    "test": "jest --coverage --logHeapUsage",
    "start": "webpack-dev-server",
    "release": "standard-version",
    "ct": "git-cz -a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xucongli1989/x-js-kit.git"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "javascript",
    "toolkit",
    "js",
    "helper",
    "library"
  ],
  "author": "XCL",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/xucongli1989/x-js-kit/issues"
  },
  "homepage": "https://github.com/xucongli1989/x-js-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.0",
    "@babel/preset-env": "^7.3.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "commitizen": "^3.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "localStorage": "^1.0.4",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
